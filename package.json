{
  "name": "robot-caml-jockey",
  "version": "1.0.0",
  "description": "Automatic OCaml tasks",
  "author": "Jon Ludlam <jon@recoil.org>",
  "license": "ISC",
  "repository": "https://github.com//robot-caml-jockey.git",
  "homepage": "https://github.com//robot-caml-jockey",
  "bugs": "https://github.com//robot-caml-jockey/issues",
  "keywords": [
    "probot",
    "github",
    "probot-app"
  ],
  "scripts": {
    "dev": "nodemon",
    "start": "probot run ./index.js",
    "lint": "standard --fix",
    "test": "jest && standard",
    "test:watch": "jest --watch --notify --notifyMode=change --coverage"
  },
  "dependencies": {
    "bs-node": "github:reasonml-community/bs-node",
    "nodegit": "^0.24.1",
    "probot": "^7.2.0"
  },
  "devDependencies": {
    "bs-platform": "^4.0.18",
    "jest": "^24.0.0",
    "nock": "^10.0.0",
    "nodemon": "^1.17.2",
    "smee-client": "^1.0.2",
    "standard": "^12.0.1"
  },
  "engines": {
    "node": ">= 8.3.0"
  },
  "standard": {
    "env": [
      "jest"
    ]
  },
  "nodemonConfig": {
    "exec": "npm start",
    "watch": [
      ".env",
      "."
    ]
  },
  "jest": {
    "testEnvironment": "node"
  }
}
